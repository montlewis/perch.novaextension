<?xml version="1.0" encoding="UTF-8"?>
<syntax name="perchCMS.php">
	<meta>
		<name>Perch Config</name>
		<type>markup</type>
		<preferred-file-extension>php</preferred-file-extension>
		<parent>php</parent>
	</meta>
	<detectors>
		<combo priority="1.0">
			<extension>php</extension>
			<filename priority="1.0">config</filename>
		</combo>
		</detectors>
</syntax>
